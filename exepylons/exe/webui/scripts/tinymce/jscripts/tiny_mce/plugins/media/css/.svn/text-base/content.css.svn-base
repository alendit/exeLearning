.mceItemFlash, .mceItemShockWave, .mceItemQuickTime, .mceItemWindowsMedia, .mceItemRealMedia, .mceItemMP3, .mceItemFlowPlayer {
	border: 1px dotted #cc0000;
	background-position: center;
	background-repeat: no-repeat;
	background-color: #ffffcc;
}

.mceItemFlowPlayer {
	background-image: url('../images/flv.gif');
}
.mceItemMP3 {
	background-image: url('../images/mp3.gif');
}
.mceItemShockWave {
	background-image: url('../images/shockwave.gif');
}

.mceItemFlash {
	background-image: url('../images/flash.gif');
}

.mceItemQuickTime {
	background-image: url('../images/quicktime.gif');
}

.mceItemWindowsMedia {
	background-image: url('../images/windowsmedia.gif');
}

.mceItemRealMedia {
	background-image: url('../images/realmedia.gif');
}
